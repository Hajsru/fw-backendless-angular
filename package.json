    "@types/node": "^10.12.11",
    "babel-preset-env": "^1.7.0",
    "codelyzer": "~4.5.0",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "jest-preset-angular": "^6.0.1",
    "lint-staged": "^8.1.0",
    "ng-packagr": "^4.2.0",
    "prettier": "^1.15.3",
