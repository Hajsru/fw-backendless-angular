{
  "*.ts": [
    "prettier --write",
    "tslint --fix",
    "git add"
  ],
  "*.scss": [
    "prettier --write",
    "stylelint --fix",
    "git add"
  ],
  "*.{jpg,jpeg}": [
    "jpegoptim --strip-all --all-progressive -pm85",
    "git add"
  ],
  "*.png": [
    "optipng -strip all -o4",
    "git add"
  ],
  "*.svg": [
    "svgo",
    "git add"
  ]
}
